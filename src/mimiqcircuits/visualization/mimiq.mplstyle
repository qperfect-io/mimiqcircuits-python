### TICKS
xtick.labelsize: 11
xtick.direction: in
xtick.color: abb2bf
ytick.labelsize: 11
ytick.direction: in
ytick.color: abb2bf

### GRIDS
grid.color: 505868
grid.linestyle: -
grid.alpha: 0.75

### LEGEND
legend.frameon: True
legend.framealpha: 0.75
legend.fancybox: True
legend.shadow: True
legend.scatterpoints: 1
