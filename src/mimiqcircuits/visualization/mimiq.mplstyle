axes.prop_cycle: cycler('color', ['e5c07b', 'c678dd', '0c7e8f', '98c379', '56b6c2', 'e06c75', '61afef'])

### LINES
lines.color: C0
lines.linewidth: 1.0
lines.markeredgewidth: 0.5
lines.markersize: 5.0
lines.dashed_pattern: 2.6, 1.4
lines.dashdot_pattern: 4.6, 1.4, 0.6, 1.4
